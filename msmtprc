defaults
auth on
tls on
tls_trust_file /usr/local/etc/openssl/cert.pem

account sundhall.se
host mail.messagingengine.com
user patrik@sundhall.se
from patrik@sundhall.se
port 587

account smspark.se
host smtp.gmail.com
user patrik@smspark.se
from patrik@smspark.se
port 587

account default : sundhall.se
